package castingTask;

public class CastingTask02 {
   public static void main(String[] args) {
//      "8.43" + 2.59를 더해서 10이 나오도록 출력하기
	   
	   String data = "", result = "";
	   double number = 0.0;
	   int dataResult = 0,numberResult = 0;
	   
	   data = "8.43";
	   number = 2.59;
	   dataResult = Integer.parseInt(data);
			   
	   numberResult = dataResult + (int)number;
	   
	   result = "%f + (int)%f = %d";
	   
	   System.out.printf(result, dataResult, number, numberResult);
	   
	   
   }
}
